}
.table-pad {
	-webkit-user-select:none;
}
.table-ipn {
	display: none;
	-webkit-user-select:none;
}
.fold-h4 {
	text-align: left;
	text-indent: -2.65em;
	padding-left: 2.65em;
	word-break: break-all;
}
.fold-h4-2 {
	text-align: left;
	text-indent: -3.3em;
	padding-left: 3.3em;
	word-break: break-all;
}
.fold-dian {
	text-align: left;
	text-indent: -0.5em;
	padding-left: 0.5em;
	word-break: break-all;
}
.top20-j {
	margin-top: 30px;
    text-align: center;
}
.sheet {
	border: 1px solid #456177;
	width: 100%;
    height: 60px;
    margin-top: 30px;
    font-size: 18px;
    color: #fff;
    -webkit-user-select: none;
    position: relative;
}
.sheet-images img {
	width: 30px;
    height: auto;
    display: block;
    position: absolute;
    left: 8px;
    top: 18px;
}
.sheet-name {
	padding-left: 41px;
    box-sizing: border-box;
    height: 60px;
    display: table-cell;
    vertical-align: middle;
    color: #456177;
    font-size: 16px;
}
.sheet-images-right img {
	width: 12px;
    height: auto;
    display: block;
    position: absolute;
    right: 8px;
    top: 19px;
}
.fold-o {
	text-align: left;
	padding-left: 1.1rem;
	text-indent: -1.1rem;	
}
.box-example-pad {
    margin: 0 auto;
    width: 620px;
}
.box-example-ph {
    display: none;
}
.example {
    width: 204px;
    height: 120px;
    line-height: 120px;
    text-align: center;
    float: left;
    margin-left: 2px;
    margin-top: 4px;
    background-color: #d48744;
    font-weight: bold;
    font-size: 20px;
    color: #fff;
}
.example > img {
    width: 100%;
    height: 120px;
    box-sizing: border-box;
}
.dialogue {
	width: 100%;
	padding: 30px 20px 30px 20px;
	margin-top: 30px;
	background-color: #f2e6ec;
	box-sizing: border-box;
}
.dialogue .character-a {
	text-indent: -4.3rem;
	padding-left: 4.3rem;
	word-break: break-all;
}
.dialogue .character-b {
	text-indent: -1.85rem;
	padding-left: 1.85rem;
	word-break: break-all;
}
.dialogue .character-talk {
	font-family: kaiti;
	border-bottom: 1px solid #d5d8dc;
}
.alternately {
	width: 100%;
	border: 2px solid #85003c;
	padding: 20px;
	margin-top: 30px;
	box-sizing: border-box;
	-webkit-user-select:none;
}

.alternately>div:first-child {
	color: #85003c;
	font-weight: bold;
}
.alternately-o {
	background-color: #85003c;
	border-radius: 50%;
	color: #fff;
	width: 22px;
    height: 22px;
    display: inline-block;
    text-indent: 0;
    line-height: 22px;
    text-align: center;
}
.alternately-cross {
	margin-top: 30px;
	width: 95px;
	background-color: #85003c;
	height: 30px;
	text-align: center;
	color: #fff;
	margin-right: 0px;
	margin-left: auto;
}
.alternately-cross:active{
	opacity: 0.6;
}
.choose {
	text-indent: -1.55rem;
	padding-left: 1.55rem;
	text-align: left;
}
.chooseOp:active {
	opacity: 0.6;
}
.star-a {
	text-indent: -2.8rem;
	padding-left: 2.8rem;
	text-align: left;
}
.jump-exercise {
	height: 60px;
    width: 100%;
    box-sizing: border-box;
    margin-top: 30px;
}
.jump-title {
    font-size: 18px;
    padding: 21px 0 21px 20px;
    border: 1px solid #85003c;
    border-radius: 6px;
    line-height: 100%;
    color: #000;
}
.jump-left {
    width: 35px;
    height: 61px;
    background-color: #85003c;
    border-radius: 0 6px 6px 0;
    float: right;
    margin-top: -61px;
    background-image: url(../images/anniu-s.png);
    background-size: 11px 24px;
    background-repeat: no-repeat;
    background-position: 12px 19px;
}




