@charset "UTF-8";
/*css document*/
/*common css*/
@font-face {
	font-family: kaiti;
	src: url("../font/kaiti.ttf");
}
@font-face {
	font-family: lantinghei;
	src: url("../font/lantinghei.ttf");
	}
	@font-face {
		font-family: lantingxihei;
		src: url("../font/lantingxihei.ttf");
	}
@font-face {
		font-family: songti;
		src: url("../font/songti.ttf");
	}
/* { font-size:18px; font-family: sans-serif;}*/
body { margin: 0px; padding: 0px; padding-left: 50px; }
div,p,ul,li,span,h1,h2,h3,h4,h5,h6 { margin:0px; padding: 0px; font-family: sans-serif; font-weight: normal;}
article, aside, details, figcaption, figure, footer, header, hgroup, menu, nav, section { 
	font-family: 
	sans-serif; 
	margin: 0px ; 
	padding: 0px; 
	font-size: 18px; 
}
img { -webkit-user-select:none; border-width: 0px; display:block;}
div { text-align: justify; text-justify:inter-ideograph; line-height: 167%; }

.section-sep{
	height: 20px;
	-webkit-user-select:none;
}
.foot{
	height: 50px;
	-webkit-user-select:none;
}
article {
	margin: 0px 48px 0px 50px;
}
section {text-indent: 2em; text-align: justify; text-justify:inter-ideograph;letter-spacing: 0.6px; margin-bottom: -6px;line-height: 167%; margin-top: 24px;}
.clearfix:after { clear: both; content: "."; display: block; height: 0; visibility: hidden; }
h1 {
	font-size: 24px;
	color: #85003c;
	line-height: 135%;
	padding: 47px 0 0px 0px;
	letter-spacing: 0.6px;
	margin-bottom: -3px;
	text-indent: -3.4em;
	padding-left: 3.4em;
}


h2 {
	font-size: 20px;
	color: #85003c;
	line-height: 135%;
	letter-spacing: 0.6px;
	padding: 27px 0px 0px 0px;
	text-indent: -1.6em;
	padding-left: 1.6em;
	margin-bottom: -3px;
}


h3 {
	font-size: 18px;
	color: #85003c;
	line-height: 135%;
	letter-spacing: 0.6px;
	padding: 27px 0px 0px 0px;
	text-indent: -2.6em;
	padding-left: 2.6em;
	margin-bottom: -3px;
}
h1 div,h2 div,h3  div{
	display: table-cell;
	
}

h4 {
	font-size: 18px;
	color: #000000;
	line-height: 167%;
	letter-spacing: 0.6px;
	padding: 0px 0px 0px 0px;
	margin-bottom: -6px;
	margin-top: 24px;
	font-weight: bold;
}
.wordhang + h4{
	margin-top: 24px;
}
h5 {
	font-size: 18px;
	color: #000000;
	letter-spacing: 0.6px;
	text-align: center;
	padding-top: 15px;
}
.mr{ 
	font-weight: bold;
	margin-bottom: -6px;
}
.number{
	display: table-cell;
	font-size: 18px;

}
.objtext{
	display: table-cell;
	font-size: 18px;
}
section + .numbox{
	margin-top: 24px;

}
.numbox{
	margin-bottom: -6px;
	margin-top: 6px;
}
sup {
	font-family: inherit;
	font-size: 80%;
	line-height: 80%;
}
sub {
	font-family: inherit;
	font-size: 80%;
	line-height: 80%;
}

.chapter {
	-webkit-user-select:none;
}
.chapter img {
	width: 718px;
	height: 1004px;
}
.chapter-box {
	height: 180px;
	-webkit-user-select:none;
	background-color: #85003c;
	width: 718px;
}
.chapter-num {
	display: inline-block;
    padding-left: 50px;
    font-size: 36px;
    color: #fff;
    margin-top: 60px;
    font-weight: bold;
    height: 60px;
    border-right: 2px solid #fff;
    padding-right: 30px;
}
.chapter-name {
    float: right;
    font-size: 36px;
    color: #fff;
   	width: 430px;
   	margin-right: 30px;
   	margin-top: 60px;
   	font-weight: bold;
}
.indent-title {
    text-indent: -1.55rem;
    padding-left: 1.55rem;
    text-align: left;
}
.indent-title-2 {
    text-indent: -2.2rem;
    padding-left: 2.2rem;
    text-align: left;
}
.has-large-image img {
    margin: 0 auto;
}
.img-caption {
    padding-top: 13px;
    color: #456177;
}
.case-box {
	width: 100%;
    background-color: #f9f2f5;
    margin-top: 30px;
    padding: 30px 20px 30px 20px;
    box-sizing: border-box;
    
}
.case-box section {
	font-family: songti;
	font-size: 16px;
}
.case-title {
	padding-left: 15px;
	line-height: 180%;
	height: 30px;
	margin: 0 auto;
	background-color: #c2809e;
	color: #fff;
	font-weight: bold;
	font-size: 18px;
	-webkit-user-select:none;
}
.case-fold {
	text-indent: -1.4rem;
    padding-left: 1.4rem;
    text-align: left;
}
.related-information {
	background-color: #85003c;
	width: 100%;
    height: 80px;
    margin-top: 30px;
    font-size: 18px;
    color: #fff;
    -webkit-user-select: none;
    position: relative;
}
.related-text {
	padding-left: 20px;
    box-sizing: border-box;
    height: 80px;
    display: table-cell;
    vertical-align: middle;
}
.related-images img {
	width: 60px;
    height: auto;
    display: block;
    position: absolute;
    right: 20px;
    top: 10px;
}
.additional-knowledge {
	background-color: #d48744;
	width: 100%;
    height: 80px;
    margin-top: 30px;
    font-size: 18px;
    color: #fff;
    -webkit-user-select: none;
    position: relative;
}
.additional-text {
	padding-left: 20px;
    box-sizing: border-box;
    height: 80px;
    display: table-cell;
    vertical-align: middle;
}
.additional-images img {
	width: 60px;
    height: auto;
    display: block;
    position: absolute;
    right: 20px;
    top: 10px;
}
.table-header {
	color: #85003c;
	text-align: center;
	line-height: 120%;
	-webkit-user-select:none;
	font-size: 16px;
	margin-top: 30px;
}
table {
	margin-top: 15px;
	width: 100%;
	-webkit-user-select:none;
}
table td {
	word-break: break-all;
	text-align: justify;
}
.table-o {
	text-align: left;
	padding-left: 0.9rem;
	text-indent: -0.9rem;
	line-height: 120%;
	font-family: lantingxihei;
	font-size: 16px;
	margin-top: 3px;
}
.table-h4 {
	text-align: left;
	padding-left: 2.4rem;
	text-indent: -2.4rem;
	line-height: 120%;
	font-family: lantingxihei;
	font-size: 16px;
	margin-top: 3px;
}
.table-h4-2 {
	text-align: left;
	padding-left: 2.9rem;
