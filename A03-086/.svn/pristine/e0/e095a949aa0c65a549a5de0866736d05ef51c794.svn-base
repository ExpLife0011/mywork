/* User selects right answer */
.slot.right::after{
    content: " ";
    position: absolute;
     width: 21px;
    height: 18px;

    right: 3px;
    bottom: 5px;
    opacity: 0.8;
    background-image: url(../images/d.png);
     background-size: 21px 18px;


    z-index: 5;
}

/*  User selects wrong answer  */
.slot.wrong::after{
    content: " ";
    position: absolute;
    width: 21px;
    height: 18px;
    z-index: 5;
    right: 3px;
    bottom: 5px;
    opacity: 0.8;
    background-image: url(../images/c.png);
    background-size: 21px 18px;

}

